<web-app>

  <!-- Librairies de tags -->
  <taglib>
    <taglib-uri>/WEB-INF/tld/template.tld</taglib-uri>
    <taglib-location>/WEB-INF/tld/template.tld</taglib-location>
  </taglib>

  <!-- Liste des Servlets -->
  <servlet> 
    <servlet-name>CConsultationTache</servlet-name> 
    <servlet-class>owep.controle.CConsultationTache</servlet-class> 
  </servlet> 
  <servlet-mapping> 
    <servlet-name>CConsultationTache</servlet-name> 
    <url-pattern>/Tache/FListeTacheVisu</url-pattern> 
  </servlet-mapping> 

</web-app>
